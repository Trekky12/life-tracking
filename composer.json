{
    "name": "trekky12/life-tracking",
    "description": "record android location with tasker, record finances, fuel consumption",
    "homepage": "http://www.haegi.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Haegenlaeuer",
            "homepage": "http://www.haegi.org/"
        }
    ],
    "require": {
        "php": ">=8.4",
        "slim/slim": "4.*",
        "slim/psr7": "^1.1.0",
        "slim/http": "^1.0",
        "slim/twig-view": "^3.0.0",
        "slim/csrf": "^1.3",
        "slim/flash": "^0.4.0",
        "php-di/php-di": "^7.0",
        "dflydev/fig-cookies": "^3.0",
        "twig/twig": "^3.0",
        "monolog/monolog": "^3.8",
        "phpmailer/phpmailer": "^6.1",
        "intervention/image": "^3.10",
        "michelf/php-markdown": "^2.0",
        "hashids/hashids": "^5.0",
        "phpoffice/phpspreadsheet": "^3.7",
        "symfony/console": "^7.2",
        "robthree/twofactorauth": "^3.0",
        "endroid/qr-code": "^6.0",
        "minishlink/web-push": "^9.0",
        "phpoffice/phpword": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.2",
        "phpunit/phpunit": "^11.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit tests --colors --verbose"
    }
}
