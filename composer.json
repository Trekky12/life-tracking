{
    "name": "trekky12/life-tracking",
    "description": "record android location with tasker, record finances, fuel consumption",
    "homepage": "http://www.haegi.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Haegenlaeuer",
            "homepage": "http://www.haegi.org/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "slim/slim": "4.*",
        "slim/psr7": "^1.1.0",
        "slim/http": "^1.0",
        "slim/twig-view": "^3.0.0",
        "slim/csrf": "^1.0",
        "slim/flash": "^0.4.0",
        "php-di/php-di": "^6.0",
        "dflydev/fig-cookies": "^3.0",
        "twig/twig": "^3.0",
        "monolog/monolog": "^2.1",
        "phpmailer/phpmailer": "^6.1",
        "intervention/image": "^2.5",
        "michelf/php-markdown": "^1.8",
        "hashids/hashids": "^4.0",
        "phpoffice/phpspreadsheet": "^1.9",
        "symfony/console": "^5.1",
        "robthree/twofactorauth": "^1.7",
        "endroid/qr-code": "^4.1",
        "minishlink/web-push": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
